#include <stdio.h>
#include <stdlib.h>

int main()
{

  char c;

  printf("Enter a character::");
  scanf("%c",&c);

  if(c>='a' && c<='z'){
    printf("%c is an Alphabet.",c);

  }

   else if(c>='A' && c<='Z'){
     printf("%c is an Alphabet.",c);
   }

   else{
    printf("%c is not an Alphabet.",c);
   }


    return 0;
}
