#include <stdio.h>


int main()
{
    //Nested if else;

    int n1,n2,n3;

    printf("Enter three numbers:");
    scanf("%d %d %d",&n1,&n2,&n3);

    if(n1>n2){
        if(n1>3){
            printf("%d is the largest",n1);

        }
        else{
            printf("%d is the largest",n3);
        }
    }
    else{
        if(n2>n3){
            printf("%d is the largest",n2);

        }
        else{
            printf("%d is the largest",n3);
        }
    }

    return 0;
}
